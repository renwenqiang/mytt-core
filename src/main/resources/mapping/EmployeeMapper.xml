<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.mytt.core.mapper.EmployeeMapper">

    <select id="getAll" resultType="com.boot.mytt.core.pojo.EmployeePOJO">
        select
        id,
        name,
        age,
        guid
        from employee
    </select>
</mapper>
